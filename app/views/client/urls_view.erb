<h3>Client: <%= @client.identifier.capitalize %></h3>
<h3>Path: <%= @url.url_path %></h3>
<p>Average Response Time: <%= @url.payload_requests.average_response_time.round(2) %></p>
<p>Max Response Time: <%= @url.payload_requests.max_response_time %></p>
<p>Min Response Time: <%= @url.payload_requests.min_response_time %></p>
<p>Most Used Request Type: <%= @url.payload_requests.most_used_request_type %></p>
<p>Screen Resolution Report: <%= @url.payload_requests.get_screen_resolution_report(url_id: @url.id) %></p>
<p>All Response Times: <%= @url.payload_requests.listed_response_times %></p>
<p>Top Referrers: <%= @url.payload_requests.top_referrers %></p>
<p>Top User Agents: <%= @url.payload_requests.top_agents %></p>
<a href="/sources/<%= @client.identifier %>">Back to <%= @client.identifier.capitalize %></a>


<%= breakdown_table(@url.payload_requests.os_breakdown(url_id: @url.id), "OS") %>
<%= breakdown_table(@url.payload_requests.browser_breakdown(url_id: @url.id), "Browser") %>
<%= breakdown_table(@url.payload_requests.all_http_verbs(url_id: @url.id), "HTTP Verbs") %>
